{
  "name": "musical-disco",
  "version": "1.0.0",
  "description": "This app is to teach how to work with react",
  "main": "index.js",
  "scripts": {
    "watch": "watchify -t [ babelify --presets [ es2015 react ] ] main.js -o ./dist/bundle.js -v",
    "reload": "browser-sync start --server --files \"./dist/bundle.js , ./css/*.css\"",
    "start": "start cmd /k \"npm run watch\" && npm run reload"
  },
  "author": "IBM-DTeam",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  }
}
